local player = game.Players.LocalPlayer

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Title of the library", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

--[[
Name = <string> - The name of the UI.
HidePremium = <bool> - Whether or not the user details shows Premium status or not.
SaveConfig = <bool> - Toggles the config saving in the UI.
ConfigFolder = <string> - The name of the folder where the configs are saved.
IntroEnabled = <bool> - Whether or not to show the intro animation.
IntroText = <string> - Text to show in the intro animation.
IntroIcon = <string> - URL to the image you want to use in the intro animation.
Icon = <string> - URL to the image you want displayed on the window.
CloseCallback = <function> - Function to execute when the window is closed.
]]

local Tab = Window:MakeTab({
	Name = "Tab 1",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--[[
Name = <string> - The name of the tab.
Icon = <string> - The icon of the tab.
PremiumOnly = <bool> - Makes the tab accessible to Sirus Premium users only.
]]

local Section = Tab:AddSection({
	Name = "Section"
})

--[[
Name = <string> - The name of the section.
]]

OrionLib:MakeNotification({
	Name = "Mubeen Hub",
	Content = "Notification content... what will it say??",
	Image = "rbxassetid://4483345998",
	Time = 5
})

--[[
Title = <string> - The title of the notification.
Content = <string> - The content of the notification.
Image = <string> - The icon of the notification.
Time = <number> - The duration of the notfication.
]]

Tab:AddButton({
	Name = "Speed!",
	Callback = function()
      		player.Character.Humanoid.WalkSpeed = 200
  	end    
})

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]

Tab:AddButton({
	Name = "Auto Farm!",
	Callback = function()
      	        local TweenService = game:GetService("TweenService") local CheatDestroy = false local Player = game.Players.LocalPlayer function InstantWin() repeat task.wait() until Player.Character ~= nil local Character = Player.Character local TempPart = Instance.new("Part") TempPart.Position = Vector3.new(-50,60,0) local TempPartTween = TweenService:Create(TempPart,TweenInfo.new(5,Enum.EasingStyle.Linear,Enum.EasingDirection.InOut),{Position = Vector3.new(-50,60,8500)}) TempPartTween:Play() repeat task.wait() if CheatDestroy == false then Character:PivotTo(TempPart.CFrame) end until TempPart.Position == Vector3.new(-50,60,8500) local TempPartTween2 = TweenService:Create(TempPart,TweenInfo.new(1,Enum.EasingStyle.Linear,Enum.EasingDirection.InOut),{Position = Vector3.new(-50,-360,9500)}) TempPartTween2:Play() repeat task.wait() if CheatDestroy == false then Character:PivotTo(TempPart.CFrame) end until TempPart.Position == Vector3.new(-50,-360,9500) end game.Lighting.GlobalShadows = false local ScreenGui = Instance.new("ScreenGui",Player.PlayerGui) ScreenGui.ScreenInsets = Enum.ScreenInsets.DeviceSafeInsets ScreenGui.ResetOnSpawn = false local Frame = Instance.new("Frame",ScreenGui) Frame.BackgroundColor3 = Color3.new(0,0,0) Frame.BackgroundTransparency = 1 Frame.Size = UDim2.new(1,0,1,0) local FrameTween = TweenService:Create(Frame,TweenInfo.new(1,Enum.EasingStyle.Circular,Enum.EasingDirection.Out,0,false),{BackgroundTransparency = 0.5}) local FrameTween2 = TweenService:Create(Frame,TweenInfo.new(1,Enum.EasingStyle.Circular,Enum.EasingDirection.In,0,false),{BackgroundTransparency = 1}) local TextLabel = Instance.new("TextLabel",Frame) TextLabel.BackgroundTransparency = 1 TextLabel.Position = UDim2.new(0.1,0,0.2,0) TextLabel.Size = UDim2.new(0.8,0,0.8,0) TextLabel.Font = Enum.Font.Oswald TextLabel.Text = "Auto farming." TextLabel.TextColor3 = Color3.new(1,1,1) TextLabel.TextTransparency = 1 TextLabel.TextScaled = true TextLabel.TextYAlignment = Enum.TextYAlignment.Top local TextLabelTween = TweenService:Create(TextLabel,TweenInfo.new(1,Enum.EasingStyle.Circular,Enum.EasingDirection.Out,0,false),{TextTransparency = 0,Position = UDim2.new(0.1,0,0.1,0)})local TextLabelTween2 = TweenService:Create(TextLabel,TweenInfo.new(1,Enum.EasingStyle.Circular,Enum.EasingDirection.In,0,false),{TextTransparency = 1,Position = UDim2.new(0.1,0,0.2,0)}) local TextButton = Instance.new("TextButton",Frame) TextButton.BackgroundTransparency = 1 TextButton.Position = UDim2.new(0.1,0,0,0) TextButton.Size = UDim2.new(0.8,0,0.8,0) TextButton.Font = Enum.Font.Oswald TextButton.Text = "Click here to stop auto farm." TextButton.TextColor3 = Color3.new(1,1,1) TextButton.TextTransparency = 1 TextButton.TextScaled = true TextButton.TextYAlignment = Enum.TextYAlignment.Bottom local TextButtonTween = TweenService:Create(TextButton,TweenInfo.new(1,Enum.EasingStyle.Circular,Enum.EasingDirection.Out,0,false),{TextTransparency = 0.5,Position = UDim2.new(0.1,0,0.1,0)}) local TextButtonTween2 = TweenService:Create(TextButton,TweenInfo.new(1,Enum.EasingStyle.Circular,Enum.EasingDirection.In,0,false),{TextTransparency = 1,Position = UDim2.new(0.1,0,0,0)}) FrameTween:Play() task.wait(0.1) TextLabelTween:Play() task.wait(0.1) TextButtonTween:Play() TextButton.MouseButton1Click:Connect(function() if CheatDestroy == false then TextButtonTween2:Play() task.wait(0.1) TextLabelTween2:Play() task.wait(0.1) FrameTween2:Play() task.wait(1) ScreenGui:Destroy() CheatDestroy = true end end) InstantWin() Player.CharacterAdded:Connect(InstantWin)
  	end    
})

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]
